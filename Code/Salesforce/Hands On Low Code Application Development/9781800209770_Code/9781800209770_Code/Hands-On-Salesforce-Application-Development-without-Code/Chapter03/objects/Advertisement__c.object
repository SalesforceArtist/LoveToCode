    <fields>
        <fullName>Customer__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>the customer who purchased the ad</description>
        <externalId>false</externalId>
        <label>Customer</label>
        <referenceTo>Account</referenceTo>
        <relationshipLabel>Advertisements</relationshipLabel>
        <relationshipName>Advertisements</relationshipName>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Deal__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>the opportunity related to this advertisement</description>
        <externalId>false</externalId>
        <label>Deal</label>
        <referenceTo>Opportunity</referenceTo>
        <relationshipLabel>Advertisements</relationshipLabel>
        <relationshipName>Advertisements</relationshipName>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Expected_Dismission_Date__c</fullName>
        <description>date when the ad should be dismissed</description>
        <externalId>false</externalId>
        <formula>ADDMONTHS(
  Installation_Date__c ,
  IF(
    ISBLANK(Monhts_of_availability__c),
    0,  
    Monhts_of_availability__c)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Expected Dismission Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Installation_Address__c</fullName>
        <description>the whole address of installation in plain text</description>
        <externalId>false</externalId>
        <label>Installation Address</label>
        <length>2000</length>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>LongTextArea</type>
        <visibleLines>5</visibleLines>
    </fields>
    <fields>
        <fullName>Installation_Date__c</fullName>
        <description>the date when ad is installed</description>
        <externalId>false</externalId>
        <label>Installation Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Is_Active__c</fullName>
        <defaultValue>false</defaultValue>
        <description>ad is still in place or not</description>
        <externalId>false</externalId>
        <label>Is Active</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Monhts_of_availability__c</fullName>
        <description>number of months the ad will be active</description>
        <externalId>false</externalId>
        <label>Monhts of availability</label>
        <precision>3</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Price__c</fullName>
        <description>the amount paid by the customer</description>
        <externalId>false</externalId>
        <label>Price</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Renewal_Status__c</fullName>
        <description>if the advertisement is about to be dismissed (or has already been dismissed) this field represents which is the renewal status</description>
        <externalId>false</externalId>
        <label>Renewal Status</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>None</fullName>
                    <default>true</default>
                    <label>None</label>
                </value>
                <value>
                    <fullName>Renewing</fullName>
                    <default>false</default>
                    <label>Renewing</label>
                </value>
                <value>
                    <fullName>Not Interested</fullName>
                    <default>false</default>
                    <label>Not Interested</label>
                </value>
                <value>
                    <fullName>To be recalled</fullName>
                    <default>false</default>
                    <label>To be recalled</label>
                </value>
                <value>
                    <fullName>Renewed</fullName>
                    <default>false</default>
                    <label>Renewed</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
