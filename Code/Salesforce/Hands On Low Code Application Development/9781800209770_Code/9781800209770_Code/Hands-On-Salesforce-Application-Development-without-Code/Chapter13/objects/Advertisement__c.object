    <validationRules>
        <fullName>Check_Approval_Status_on_Activation</fullName>
        <active>true</active>
        <description>Check approval status before activating the advertisement</description>
        <errorConditionFormula>AND(
					ISCHANGED(Is_Active__c),
					Is_Active__c = true,
				 Discount__c &gt; 0,
				 Price__c &gt; 0,
					NOT(ISPICKVAL(Discount_Approved__c, &apos;Approved&apos;))
)</errorConditionFormula>
        <errorMessage>You cannot activate the advertisement if record has not been submitted for approval and approved</errorMessage>
    </validationRules>
